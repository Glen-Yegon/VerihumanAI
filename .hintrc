{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "user-select",
          "scrollbar-width",
          "scrollbar-color",
          "scrollbar-width: thin",
          "overscroll-behavior",
          "-webkit-mask",
          "overscroll-behavior: contain"
        ]
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "video[playsinline]"
        ]
      }
    ]
  }
}